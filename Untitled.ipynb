   "source": [
    "def string_match(a, b):\n",
    "  counts = 0\n",
    "  for aux in range(len(a)-1):\n",
    "        print('a :'+a[aux:aux+2])\n",
    "        print('b: '+b[aux:aux+2])\n",
    "        if a[aux:aux+2] == b[aux:aux+2]:\n",
    "            counts += 1\n",
    "  \n",
    "  return counts"
   ]
